    "jm-castle-types": "^0.x",
    "react": "^18.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.1",
    "jm-castle-types": "^0.3.2",
    "prettier": "^2.7.1",
